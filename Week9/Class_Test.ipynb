    "    def currCount(self):\n",
    "  \t    print(self.count)\n",
    "    def incr(self):\n",
    "  \t    self.count += 1\n",
    "    def reset(self):\n",
    "  \t    self.count = 0\n",
    "\n",
    "c1 = Counter(1)\n",
    "\n",
    "c1.incr()\n",
    "c1.currCount()\n"
